{
	"name": "exports-mappings-and-conditions",
	"main": "index.js",
	"exports": {
		".": {
			"require": "./index.js'",
			"module": "./index-mapped.js"
		},
		"./foo": {
			"node": {
				"require": "./foo.js",
				"default": "./foo.js"
			},
			"module": {
				"require": "./foo.js",
				"default": "./foo-mapped.js"
			}
		},
		"./bar": "./bar-mapped.js"
	}
}
