{
  "name": "@rollup/plugin-strip",
  "version": "1.2.2",
  "description": "Remove debugger statements and functions like assert.equal and console.log from your code.",
  "license": "MIT",
  "repository": "rollup/@rollup/plugin-strip",
  "author": "Rich Harris",
  "homepage": "https://github.com/rollup/@rollup/plugin-strip#readme",
  "bugs": {
    "url": "https://github.com/rollup/@rollup/plugin-strip/issues"
  },
  "main": "dist/@rollup/plugin-strip.cjs.js",
  "scripts": {
    "build": "rollup -c",
    "ci:coverage": "nyc pnpm run test && nyc report --reporter=text-lcov > coverage.lcov",
    "ci:coverage:submit": "curl -s https://codecov.io/bash | bash -s - -F beep",
    "ci:lint": "pnpm run lint && pnpm run security",
    "ci:lint:commits": "commitlint --from=${CIRCLE_BRANCH} --to=${CIRCLE_SHA1}",
    "ci:test": "pnpm run test -- --verbose",
    "lint": "pnpm run lint:js && pnpm run lint:docs && pnpm run lint:package",
    "lint:docs": "prettier --write README.md",
    "lint:js": "eslint --fix --cache src test/test.js",
    "lint:package": "prettier --write package.json --plugin=prettier-plugin-package",
    "pretest": "pnpm run build",
    "security": "echo 'pnpm needs `npm audit` support'",
    "test": "ava"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "rollup",
    "modules",
    "debugging",
    "javascript"
  ],
  "peerDependencies": {
    "rollup": "^1.20.0"
  },
  "dependencies": {
    "estree-walker": "^0.6.0",
    "magic-string": "^0.25.1",
    "rollup-pluginutils": "^2.8.1"
  },
  "devDependencies": {
    "acorn": "^6.0.2",
    "rollup": "^1.20.0"
  },
  "ava": {
    "files": [
      "!**/fixtures/**",
      "!**/helpers/**",
      "!**/recipes/**",
      "!**/types.ts"
    ]
  },
  "module": "dist/@rollup/plugin-strip.es.js"
}
